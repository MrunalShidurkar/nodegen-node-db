{
	"coverageDirectory": "coverage",
	"collectCoverageFrom": [
	  "src/**/*.{ts,js}",
	  "!**/node_modules/**"
	],
	"transform": {
	  ".(ts|tsx)": "<rootDir>/test/preprocessor.js"
	},
	"testMatch": [
	  "**/test/**/*test.ts"
	],
	"moduleFileExtensions": [
	  "ts",
	  "tsx",
	  "js",
	  "json"
	],
	"testEnvironment": "node",
	"setupFilesAfterEnv": ["./test/unit/lib/setup.ts"],
	"verbose": true
  }
  